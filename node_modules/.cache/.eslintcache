ules":"68"},"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/index.js",["69"],[],"import React from 'react';\nimport ReactDOM from 'react-dom/client';\nimport './index.css';\nimport App from './App';\nimport {\n  HashRouter as Router,\n  Route\n} from 'react-router-dom';\nimport reportWebVitals from './reportWebVitals';\n\nconst root = ReactDOM.createRoot(document.getElementById('root'));\nroot.render(\n  <React.StrictMode>\n    <Router\n     basename='/'>\n      <App />\n    </Router>\n  </React.StrictMode>\n);\n\n// If you want to start measuring performance in your app, pass a function\n// to log results (for example: reportWebVitals(console.log))\n// or send to an analytics endpoint. Learn more: https://bit.ly/CRA-vitals\nreportWebVitals();\n",[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/reportWebVitals.js",[],[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/App.js",[],[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/Footer.js",["70"],[],"export function Footer(){\n    return(\n    <div class=\"d-flex flex-column\">\n    <div id=\"page-content\">\n        <div class=\"container text-center\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-7\">\n            <h1 class=\"fw-light mt-4 text-white\"></h1>\n            <p class=\"lead text-white-50\"></p>\n            </div>\n        </div>\n        </div>\n    </div>\n    <footer id=\"sticky-footer\" class=\"flex-shrink-0 py-4 bg-dark text-white-50\">\n        <div class=\"container text-center\">\n        <small>Copyright &copy; Peter Myer 2022</small>\n        </div>\n    </footer>\n    </div>\n    )\n}","/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/Navbar.js",[],[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/Routes.js",["71"],[],"import React from \"react\";\nimport { Routes, Route, Link } from 'react-router-dom';\nimport Home from './Home'\nimport About from \"./About\";\nimport Projects from \"./Projects\";\n// import {BookItProjectPage} from \"./BookItProjectPage\";\n// import {ByteProjectPage} from \"./ByteProjectPage\"\n\nexport default function AppRoutes(){\n\n    return(\n        <div>\n            <Routes>\n                <Route path=\"/about\" element={<About/>} />\n                {/* <Route path=\"/BookIt\" element={<BookItProjectPage/>} />\n                <Route path=\"/byte\" element={<ByteProjectPage/>} /> */}\n                <Route path=\"/projects\" element={<Projects/>} />\n                <Route path=\"/\" element={<Home/>} />\n\n            </Routes>\n        </div>\n    )\n\n}\n","/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/Home.js",["72","73"],[],"import React from \"react\";\nimport Profile from './About'\nimport CssLoader from \"./CssLoader\";\n\nexport default function Home(){\n    return(\n        <div className=\"jumbotron jumbotron-fluid\">\n            <div className = \"container row d-flex align-items-center \">\n                <div className = \"col p-5 text-center \" >\n                    <img className=\"img-fluid profile-image\" src=\"profile.jpeg\" alt=\"profile_pic\" width = \"300\" loading=\"lazy\"/>\n                </div>\n                <div className = \"col p-5   \">\n                    <h1 className =\"display-3\">Hi, I'm <strong><span className=\"text-highlight\">Peter Myer</span></strong></h1>\n                    <p className=\"lead\">Full Stack Web Developer</p>\n                    <hr className=\"my-4\"></hr>\n                    <p className = \"small text-muted\">Creating web applications with <strong>Javascript</strong> and <strong>Python</strong></p>\n                    <p className = \"small text-muted\"> Experienced in <strong>Illustration</strong> and <strong>Design</strong></p>\n                </div>\n            </div>\n        </div>\n\n    )\n}","/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/About.js",[],[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/Projects.js",[],[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/ByteProjectPage.js",[],[],"/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/BookItProjectPage.js",["74"],[],"import React, {useEffect} from \"react\";\n\nexport function BookItProjectPage(){\n\n    const openTab=(event, tabName)=>{\n    // Get all elements with class=\"tabcontent\" and hide them\n    let tabcontent = document.getElementsByClassName(\"tabcontent\");\n    for (let i = 0; i < tabcontent.length; i++) {\n        tabcontent[i].style.display = \"none\";\n    }\n  \n    // Get all elements with class=\"tablinks\" and remove the class \"active\"\n    let tablinks = document.getElementsByClassName(\"tablinks\");\n    for (let i = 0; i < tablinks.length; i++) {\n      tablinks[i].className = tablinks[i].className.replace(\" active\", \"\");\n    }\n  \n    // Show the current tab, and add an \"active\" class to the link that opened the tab\n    let thisDocument = document.getElementById(tabName)\n    thisDocument.style.display = \"block\";\n    event.currentTarget.className += \" active\";\n    }\n\n    useEffect(()=>{\n        document.getElementById(\"defaultOpen\").click()\n    },[])\n\n    return(\n        <article>\n            <div class=\"tab\">\n                <button class=\"tablinks\" id=\"defaultOpen\" onClick={(event)=>openTab( event,\"Summary\")}> Summary</button>\n                <button class=\"tablinks\" id=\"defaultOpen\" onClick={(event)=>openTab( event,\"TechStack\")}> Tech Stack</button>\n                <button class=\"tablinks\"  onClick={(event)=>openTab(event, \"Expansions\")}>Future Expansions</button>\n            </div>\n            <section>\n                <h1 className=\"display-6\">BookIt</h1>\n                <div>\n                    <a href=\"#\"><i class=\"fa-brands fa-github\"></i></a> \n                    <a href=\"https://bookit-web-app.herokuapp.com/\">Visit BookIt Web App</a> \n                </div>\n                <div id=\"Summary\" class=\"tabcontent\">\n                    <div className=\"container pt-1\">\n                        <div>In Progress</div>\n                    </div>\n                    <div className=\"container pt-3\">\n                        <p>\n                            A combination of web app and Google Chrome extension, BookIt was envisioned as a solution to the problem of backlogged articles, images, and 'someday' content that many of us have ever growing in our browser's bookmarks.\n                        </p>\n                        <p>Alongside adding pages to a user's collection, BookIt tracks the status of which articles have actually been read, and when. Additionally, users can generate a link to easily share\n                            selected content with their peers.</p>\n                    </div>\n                </div>\n                <div id=\"TechStack\" class=\"tabcontent\">\n                    <ul>\n                        <li>Built in Node.js with React and Express</li>\n                        <li>PostgresSQL DB with Sequelize ORM for managing user data</li>\n                        <li>Chrome extension created with Manifest V3</li>\n                        <li>Metascrapper.js retrieves article OpenGraph data prior to saving to database </li>\n                        <li>Ag-grid to construct table views for user and shared articles</li>\n                        <li>Plotly.js for creating user data visualizations</li>\n                    </ul>\n                </div>\n                <div id=\"Expansions\" class=\"tabcontent\">\n                    <ul>\n                        <li>Create a main page which can host featured and popular articles or categories for all users</li>\n                        <li>Provide higher order tags or folders to increate organization options</li>\n                        <li>Integrate Auth0 security</li>\n                        <li>Allow users to set goals for reading articles or tagged content</li>\n                        <li>Provide mechanism to remind, or nudge, users to hit their reading goals</li>\n                        <li>Create additional data views for interesting user metrics</li>\n                    </ul>\n                </div>\n            </section>\n        </article>\n)}","/Users/petermyer/Desktop/PortfolioProjects/PeterMyer.github.io/src/CssLoader.js",[],[],[],{"ruleId":"75","severity":1,"message":"76","line":7,"column":3,"nodeType":"77","messageId":"78","endLine":7,"endColumn":8},{"ruleId":"79","severity":1,"message":"80","line":8,"column":13,"nodeType":"81","endLine":8,"endColumn":50},{"ruleId":"75","severity":1,"message":"82","line":2,"column":25,"nodeType":"77","messageId":"78","endLine":2,"endColumn":29},{"ruleId":"75","severity":1,"message":"83","line":2,"column":8,"nodeType":"77","messageId":"78","endLine":2,"endColumn":15},{"ruleId":"75","severity":1,"message":"84","line":3,"column":8,"nodeType":"77","messageId":"78","endLine":3,"endColumn":17},{"ruleId":"85","severity":1,"message":"86","line":38,"column":21,"nodeType":"81","endLine":38,"endColumn":33},"no-unused-vars","'Route' is defined but never used.","Identifier","unusedVar","jsx-a11y/heading-has-content","Headings must have content and the content must be accessible by a screen reader.","JSXOpeningElement","'Link' is defined but never used.","'Profile' is defined but never used.","'CssLoader' is defined but never used.","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/jsx-eslint/eslint-plugin-jsx-a11y/blob/HEAD/docs/rules/anchor-is-valid.md"]